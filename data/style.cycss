{
    "style": [
        {
            "selector": "node",
            "style": {
                "label": "",
                "background-color": "#a9b6c2",
                "text-valign": "top",
                "text-halign": "center",
                "color": "#3b444d",
                "font-size" : "20",
                "text-wrap": "wrap",
                "text-max-width": "20",
                "width" : "30",
                "height": "30",
                "shape": "ellipse",
                "font-family": "Inconsolata",
                "font-weight": "bold",
                "min-zoomed-font-size": "15"
            }
        },
        {
            "selector": "edge",
            "style": {
                "line-color": "#a9b6c2",
                "width": "3",
                "target-arrow-color": "#a9b6c2",
                "target-arrow-shape": "triangle",
                "arrow-scale": "2",
                "curve-style": "bezier",
                "opacity": "0.8"
            }
        },
        {
            "selector": "node.showLabel",
            "style": {
                "label": "data(name)"
            }
        },
        {
            "selector": "node.hideLabel",
            "style": {
                "label": ""
            }
        },
        {
            "selector": "node:parent",
            "style": {
                "label": "data(name)",
                "font-size": "30",
                "font-weight": "bold",
                "text-valign": "top",
                "text-halign": "center",
                "background-color": "white",
                "color": "#3b444d",
                "border-color": "#a9b6c2",
                "border-width": "2"
            }
        },
        {
            "selector": "node[role='Controller'], .Controller",
            "style": {
                "background-color": "#755194",
                "color": "#755194"
            }
        },
        {
            "selector": "node[role='Coordinator'], .Coordinator",
            "style": {
                "background-color": "#539967",
                "color": "#539967"
            }
        },
        {
            "selector": "node[role='Interfacer'], .Interfacer",
            "style": {
                "background-color": "#E9AB45",
                "color": "#E9AB45"
            }
        },
        {
            "selector": "node[role='Information Holder'], .InformationHolder",
            "style": {
                "background-color": "#bf3f6a",
                "color": "#bf3f6a"
            }
        },
        {
            "selector": "node[role='Service Provider'], .ServiceProvider",
            "style": {
                "background-color": "#4d82b0",
                "color": "#4d82b0"
            }
        },
        {
            "selector": "node[role='Structurer'], .Structurer",
            "style": {
                "background-color": "#e6a1b2",
                "color": "#e6a1b2"
            }
        },
        {
            "selector": "node.selected",
            "style": {
                "border-color": "#3b444d",
                "border-width": "8",
                "opacity": "1"
            }
        },
        {
            "selector": "node.faded",
            "style": {
                "opacity": "0.5"
            }
        },
        {
            "selector": "edge.faded",
            "style": {
                "opacity": "0.5"
            }
        },
        {
            "selector": "node:parent.faded",
            "style": {
                "opacity": "1"
            }
        },
        {
            "selector": "node.removed",
            "style": {
                "background-color": "#a9b6c2",
                "color": "#3b444d",
                "opacity": "0.2"
            }
        },
        {
            "selector": "node:parent.removed",
            "style": {
                "background-color": "white",
                "color": "#3b444d",
                "border-color": "#a9b6c2",
                "opacity": "0.5"
            }
        },
        {
            "selector": "edge.removed",
            "style": {
                "line-color": "#a9b6c2",
                "target-arrow-color": "#a9b6c2",
                "opacity": "0.2"
            }
        },
        {
            "selector": ".hide",
            "style": {
                "display": "none"
            }
        },
        {
            "selector": ".added",
            "style": {
                "opacity": "1"
            }
        },
        {
            "selector": ".hover",
            "style": {
                "opacity": "0.1"
            }
        },
        {
            "selector": "edge.hover",
            "style": {
                "opacity": "0"
            }
        }
    ]
}